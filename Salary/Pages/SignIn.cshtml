@page
@model Salary.Pages.SignInModel
@{
}

<form method="post">
    <table class="table">
        <tr>
            <td>UserName</td>
            <td><input asp-for="@Model.account.Username" /></td>
        </tr>
        <tr>
            <td>Password</td>
            <td><input type="password" asp-for="@Model.account.Password" /></td>
        </tr>
        <tr>
            <td><input class="btn btn-success" type="submit" value="Login" /></td>
            <td><a class="btn btn-success" asp-page="signup" >SignUp</a></td>
        </tr>
      

    </table>
  
</form>